<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="HouseOwnerBusiness">


	<start-state name="业务受理">
		<transition to="收费判断"></transition>
	</start-state>


	<task-node name="受理">
		<transition to="收费判断"></transition>
	</task-node>

	<task-node name="审核">
		<transition to="登薄"></transition>
		<transition to="受理" name="to 回退"></transition>
	</task-node>

	<task-node name="登薄">
		<transition to="繕证判断"></transition>
		<transition to="受理" name="to 受理"></transition>
	</task-node>

	<task-node name="归档">
		<transition to="结束"></transition>
	</task-node>

	<task-node name="收费">
		<transition to="收费确认"></transition>
	</task-node>

	<task-node name="繕证">
		<transition to="归档"></transition>
	</task-node>

	<decision name="繕证判断">
		<transition to="繕证" name="to 繕证"></transition>
		<transition to="归档" name="to 归档"></transition>
	</decision>

	<task-node name="收费确认">
		<transition to="审核"></transition>
	</task-node>

	<decision name="收费判断">
		<transition to="收费"></transition>
		<transition to="审核" name="to 产权科审核"></transition>
	</decision>


	<end-state name="结束"></end-state>


</process-definition>