<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="HouseOwnerBusiness">


    <start-state name="业务受理">
        <transition to="受理"></transition>
    </start-state>


    <task-node name="受理">
        <task name="受理">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
            <assignment actor-id="#{ownerBusinessHome.instance.createEmpCode}"></assignment>
        </task>
        <transition to="收费判断"></transition>
    </task-node>

    <task-node name="审核">
        <task name="审核">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;,isCheck:true}
			</description>
            <assignment pooled-actors="#{businessDefineHome.instance.rolePrefix}check"></assignment>
        </task>
		<transition to="审核结果"></transition>
    </task-node>

    <task-node name="登薄">
        <task name="登薄">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
            <assignment pooled-actors="RecordFile"></assignment>
        </task>
        <transition to="登簿结果"></transition>
    </task-node>

    <task-node name="归档">
        <task name="归档">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
            <assignment pooled-actors="RecordSave"></assignment>
        </task>
        <transition to="结束"></transition>
    </task-node>

    <task-node name="收费">
        <task name="收费">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
            <assignment pooled-actors="Money"></assignment>
        </task>
        <transition to="收费确认"></transition>
    </task-node>

    <task-node name="缮证">
        <task name="缮证">
            <description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
            <assignment pooled-actors="PrintCard"></assignment>
        </task>
        <transition to="归档"></transition>
    </task-node>

    <decision name="繕证判断">
        <transition to="缮证" name="to 证">
			<condition expression="#{not calcHelper.stringIn(businessDefineHome.id,'WP37','WP42','WP43','WP46','WP4','WP8','WP12','WP17','WP21','WP73','WP74','WP84')}"></condition>
		</transition>
        <transition to="归档" name="to 归档">
			<condition expression="#{calcHelper.stringIn(businessDefineHome.id,'WP38','WP42','WP43','WP46','WP4','WP8','WP12','WP17','WP21','WP73','WP74','WP84')}"></condition>
		</transition>
    </decision>

    <task-node name="收费确认">
        <task name="收费确认">
			<description>
				{&quot;description&quot;:&quot;&quot;,&quot;operComponent&quot;:&quot;houseOwnerTaskOper&quot;}
			</description>
			<assignment actor-id="#{actor.id}"></assignment>
		</task>
		<transition to="审核"></transition>
    </task-node>

    <decision name="收费判断">
        <transition to="收费" name="to 收费">
			<condition expression="#{not calcHelper.stringIn(businessDefineHome.id,'WP38','WP43','WP46','WP4','WP8','WP12','WP17','WP21','WP73','WP74')}"></condition>
		</transition>
        <transition to="审核" name="to 审核">
			<condition expression="#{calcHelper.stringIn(businessDefineHome.id,'WP38','WP43','WP46','WP4','WP8','WP12','WP17','WP21','WP73','WP74')}"></condition>
		</transition>
    </decision>

	<decision name="审核结果">
		<transition to="登薄" name="审核通过">
			<condition expression="#{transitionType eq 'NEXT'}"></condition>
		</transition>
		<transition to="受理" name="审核不通过">
			<condition expression="#{transitionType eq 'BACK'}"></condition>
		</transition>
	</decision>

	<decision name="登簿结果">
		<transition to="受理" name="回退">
			<condition expression="#{transitionType eq 'BACK'}"></condition>
		</transition>
		<transition to="繕证判断" name="登薄完成">
			<condition expression="#{transitionType eq 'NEXT'}"></condition>
		</transition>
	</decision>


    <end-state name="结束"></end-state>


</process-definition>