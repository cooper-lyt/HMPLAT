<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" login-required="true"
      no-conversation-view-id="/func/system/business/start.xhtml"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

    <action execute="#{taskPublish.publish}"/>

    <navigation from-action="#{taskPublish.wire}">
        <rule if-outcome="success" if="#{businessDefineHome.haveNeedFile}">
            <redirect view-id="/business/houseOwner/BizStartFileUpload.xhtml"/>
        </rule>

        <rule if-outcome="success" if="#{not businessDefineHome.haveNeedFile}">
            <redirect view-id="/business/houseOwner/BizStartConfirm.xhtml"/>
        </rule>
    </navigation>
</page>