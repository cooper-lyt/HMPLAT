<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      login-required="true"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

    <restrict>#{s:hasRole('owner.RecordView')}</restrict>


    <param name="searchKey" value="#{houseRecordCondition.searchKey}"/>
    <param name="searchType" value="#{houseRecordCondition.searchTypeName}"/>

    <param name="mapNumber" value="#{houseRecordCondition.mapNumber}"/>
    <param name="blockNumber" value="#{houseRecordCondition.blockNumber}"/>
    <param name="buildNumber" value="#{houseRecordCondition.buildNumber}"/>
    <param name="houseNumber" value="#{houseRecordCondition.houseNumber}"/>

    <param name="frameNumber" value="#{houseRecordCondition.frameNumber}"/>
    <param name="cabinetNumber" value="#{houseRecordCondition.cabinetNumber}"/>
    <param name="boxNumber" value="#{houseRecordCondition.boxNumber}"/>

    <param name="cardTypeName" value="#{houseRecordCondition.cardTypeName}"/>
    <param name="credentialsTypeName" value="#{houseRecordCondition.credentialsTypeName}"/>


    <param name="firstResult" value="#{houseRecordSearch.firstResult}"/>

