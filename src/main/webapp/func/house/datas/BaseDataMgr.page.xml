<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">



    <param name="dataTypeName" value="#{baseMapDataMgr.dataTypeName}"/>
    <param name="oneSearchTypeName" value="#{baseMapDataMgr.oneSearchTypeName}"/>


    <param name="oneCondition" value="#{baseMapDataMgr.oneCondition}"/>


    <param name="operator" value="#{baseMapDataMgr.operator}"/>
    <param name="firstResult" value="#{baseMapDataMgr.firstResult}"/>


    <param name="developerSort" value="#{developerList.orderColumn}"/>
    <param name="districtSort" value="#{districtList.orderColumn}"/>
    <param name="sectionSort" value="#{sectionList.orderColumn}"/>
    <param name="projectSort" value="#{projectList.orderColumn}"/>

    <param name="developerDir" value="#{developerList.orderDirection}"/>
    <param name="districtDir" value="#{districtList.orderDirection}"/>
    <param name="sectionDir" value="#{sectionList.orderDirection}"/>
    <param name="projectDir" value="#{projectList.orderDirection}"/>

    <param name="districtId" value="#{districtHome.id}"/>
    <param name="developerId" value="#{developerHome.id}"/>


    <navigation from-action="#{baseMapDataMgr.createNew}">
        <rule if-outcome="project-create">
            <redirect view-id="/func/house/datas/ProjectEdit.xhtml"/>
        </rule>
        <rule if-outcome="section-create">
            <redirect view-id="/func/house/datas/SectionEdit.xhtml"/>
        </rule>
    </navigation>

</page>