<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

    <param name="dataType" value="#{baseMapDataMgr.dataTypeName}"/>


    <param name="oneCondition" value="#{baseMapDataMgr.oneCondition}"/>


    <param name="operator" value="#{baseMapDataMgr.operator}"/>
    <param name="firstResult" value="#{baseMapDataMgr.firstResult}"/>
    <param name="sort" value="#{baseMapDataMgr.sort}"/>
    <param name="dir" value="#{baseMapDataMgr.dir}"/>

    <navigation from-action="#{baseMapDataMgr.createNew}">
        <rule if-outcome="project-create">
            <redirect view-id="/func/house/datas/ProjectEdit.xhtml"/>
        </rule>
        <rule if-outcome="section-create">
            <redirect view-id="/func/house/datas/SectionEdit.xhtml"/>
        </rule>
    </navigation>

</page>