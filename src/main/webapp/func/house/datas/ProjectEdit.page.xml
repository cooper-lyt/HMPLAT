<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      no-conversation-view-id="/func/house/datas/BaseDataMgr.xhtml"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">


    <param name="projectId" value="#{projectHome.id}"/>
    <param name="sectionId" value="#{sectionHome.id}"/>
    <param name="developerId" value="#{developerHome.id}"/>
    <param name="districtId" value="#{districtHome.id}"/>
    <param name="sectionSearchName" value="#{sectionSearch.searchName}"/>
    <param name="newSectionName" value="#{sectionSearch.newSectionName}"/>
    <param name="developerSearchName" value="#{developerHome.searchName}"/>
    <param name="newDeveloperName" value="#{developerHome.newDeveloperName}"/>

    <navigation from-action="#{projectEdit.saveProject}">
        <rule if-outcome="persisted">
            <end-conversation />
            <redirect view-id="/func/house/datas/BaseDataMgr.xhtml">
                <param name="dataTypeName" value="PROJECT_MGR"/>
            </redirect>
        </rule>
        <rule if-outcome="updated">
            <end-conversation />
            <redirect view-id="/func/house/datas/BaseDataMgr.xhtml">
                <param name="dataTypeName" value="PROJECT_MGR"/>
            </redirect>
        </rule>
    </navigation>

</page>